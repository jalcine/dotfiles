# vi: set ft=gdb:
set confirm off
set verbose off
set logging off
set pagination off
set print pretty on
set print array on
set print array-indexes on
set print elements 5
set print repeats 2
set print null-stop on
set print symbol-filename on
set auto-load python-scripts on
#set print object on
#set print vtbl on
#set print demangle on
#set print asm-demangle on
#set overload-resolution on

source ~/.gdb/aliases.gdb
#source ~/.gdb/c++.gdb
